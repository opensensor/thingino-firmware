BR2_LINUX_KERNEL=y
BR2_LINUX_KERNEL_CUSTOM_GIT=y
BR2_LINUX_KERNEL_CUSTOM_REPO_URL="https://github.com/gtxaspec/openipc_linux.git"
BR2_LINUX_KERNEL_CUSTOM_REPO_BRANCH="ingenic-t31"
BR2_LINUX_KERNEL_CUSTOM_REPO_VERSION="$(shell git ls-remote $(BR2_LINUX_KERNEL_CUSTOM_REPO_URL) $(BR2_LINUX_KERNEL_CUSTOM_REPO_BRANCH) | head -1 | cut -f1)"
BR2_LINUX_KERNEL_USE_CUSTOM_CONFIG=y
BR2_LINUX_KERNEL_CUSTOM_CONFIG_FILE="$(BR2_EXTERNAL)/board/ingenic/xburst1/kernel/$(SOC_FAMILY).generic.config"
BR2_LINUX_KERNEL_UIMAGE=y
BR2_LINUX_KERNEL_LZMA=y
# BR2_LINUX_KERNEL_INSTALL_TARGET is not set
